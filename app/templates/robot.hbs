<div class="row">
    <div class="col-md-12">
        {{#link-to 'application'}}
            Back
        {{/link-to}}
        <form>
            {{input type="text" class="form-control" placeholder="Robot Date" value=value}}
            <button type="submit" class="btn btn-primary" {{action "getDate"}}>Get Date</button>
        </form>
        {{#if humanDate}}
            {{input type="text" class="form-control" value=humanDate}}
        {{/if}}
    </div>
</div>