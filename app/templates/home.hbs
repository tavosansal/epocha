<div class="jumbotron">
    <div class="container">
        <h1>Epocha</h1>
        <p>Easy epoch to date converter</p>

        <div class="container-fluid">
          <div class="row">
            {{#each model as |converter|}}
                {{unix-converter model=converter}}
            {{/each}}


            <form>
              <button {{action 'addConverter'}}>Add New</button>
            </form>

          </div>
        </div>
    </div>
</div>
